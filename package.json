{
  "name": "typescript-foundation",
  "version": "1.0.0",
  "description": "TypeScript foundation for distributable packages",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/sushiswap/typescript-foundation.git",
  "author": "Matthew Lilley <hello@matthewlilley.com>",
  "license": "MIT",
  "scripts": {
    "prepublish": "tsc",
    "test": "jest",
    "testDebug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "statistics": "tsc && node ./dist/src/stat.js",
    "testAlchemyAPI": "tsc && node ./dist/src/testAlchemyAPI.js",
    "debug": "tsc && node --inspect-brk ./dist/src/stat.js"
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --cache --fix",
      "jest --bail --passWithNoTests --findRelatedTests",
      "prettier --write"
    ],
    "*.md": "prettier --write"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@sushiswap/prettier-config": "^0.1.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-plugin-jest": "^24.1.5",
    "husky": ">=5",
    "jest": "^26.6.3",
    "lint-staged": ">=10",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@alch/alchemy-web3": "^1.1.9",
    "@ethersproject/bignumber": "^5.6.0",
    "@ethersproject/providers": "^5.6.5",
    "node-fetch-commonjs": "^3.1.1",
    "web3": "^1.6.1",
    "dotenv": "^16.0.0"
  },
  "engines": {
    "node": ">=14.x"
  }
}
